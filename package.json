{
  "name": "@pendulum-baas/cli",
  "version": "1.0.6",
  "main": "dist/index.js",
  "bin": {
    "pendulum": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "lib/**/*",
    "bin/**/*",
    "cdk.json",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc && npm run make && npm run build:cdk",
    "build:cdk": "tsc bin/main.ts --outDir . --target es2020 --module commonjs",
    "make": "chmod +x ./dist/index.js",
    "dev": "tsc --watch",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/aws-sdk": "^0.0.42",
    "@types/jest": "^30.0.0",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1020.2",
    "jest": "^30.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.8",
    "aws-cdk-lib": "2.202.0",
    "aws-sdk": "^2.1692.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "constructs": "^10.0.0",
    "dotenv": "^17.2.0",
    "inquirer": "^12.8.2",
    "ora": "^5.4.1"
  }
}
